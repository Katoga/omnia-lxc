[Unit]
Description=Prometheus exporter for librespeed network speed tester
After=network-online.target

[Service]
User=librespeed-exporter
Group=librespeed-exporter
ExecStart=/usr/local/bin/librespeed_exporter --data-retriever-command /usr/local/bin/speedtest-cli --librespeed.server 85

[Install]
WantedBy=multi-user.target
